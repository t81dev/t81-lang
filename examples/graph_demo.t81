fn graph_demo() -> i32 {
    let adjacency: Matrix[i32, 3, 3] = [
        [0, 1, 1],
        [1, 0, 0],
        [1, 0, 0],
    ];
    let total_edges: i32 = adjacency[0][1] + adjacency[0][2] + adjacency[1][0] + adjacency[2][0];
    return total_edges;
}

fn main() -> i32 {
    return graph_demo();
}
